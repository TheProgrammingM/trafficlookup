{

  "manifest_version": 2,
  "name": "Trafficlookup",
  "version": "1.1",

  "description": "You can see systeminformations, cookies and more. Check if someone is stealing your data. Take your Privacy back!",
  "homepage_url": "",
  "icons": {
    "64": "icons/DropboxInGut.png"
  },

  "permissions": [
    "activeTab",
    "browsingData",
    "geolocation",
    "cookies",
    "<all_urls>",
    "tabs"
  ],

  "browser_action": {
    "default_icon": "icons/DropboxInGut.png",
    "default_title": "Trafficlookup",
    "default_popup": "popup/choose_beast.html"
  }
